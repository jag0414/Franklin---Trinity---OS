# Frontend Configuration (for React/Vite)
# API Base URL for backend services (Railway deployment)
VITE_API_BASE_URL=https://franklin-trinity-os-roosevelt.up.railway.app

# AI Provider API Keys (optional - for direct AI provider access)
VITE_OPENAI_API_KEY=your-openai-api-key
VITE_ANTHROPIC_API_KEY=your-anthropic-api-key
VITE_GOOGLE_API_KEY=your-google-api-key
VITE_STABILITY_API_KEY=your-stability-api-key

# Server Configuration (for backend/Node.js)
PORT=3000
NODE_ENV=development

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/franklin-trinity-os
DB_NAME=franklin-trinity-os

# JWT Configuration
JWT_SECRET=your-secret-key-change-in-production
JWT_EXPIRES_IN=7d

# CORS Configuration
CORS_ORIGIN=http://localhost:3000

# Logging
LOG_LEVEL=info
